@model IEnumerable<AspnetIdentitySample.Models.Certificate>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountryOfOrigin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ISOCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommodityDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CountryOfOrigin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ISOCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommodityDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CertificateId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CertificateId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CertificateId })
        </td>
    </tr>
}

</table>
